{
  "name": "@suisui/desktop",
  "version": "0.1.0",
  "private": true,
  "description": "SuiSui Desktop - BDD Test Builder with Playwright and Gherkin",
  "license": "MIT",
  "homepage": "https://github.com/Asteb612/SuiSui",
  "repository": {
    "type": "git",
    "url": "https://github.com/Asteb612/SuiSui.git",
    "directory": "apps/desktop"
  },
  "author": {
    "name": "Arthur Grosso",
    "email": "arthur.grosso@gmail.com"
  },
  "keywords": [
    "bdd",
    "gherkin",
    "playwright",
    "testing",
    "electron",
    "cucumber",
    "e2e"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "pnpm build:shared && concurrently -k \"pnpm dev:nuxt\" \"pnpm dev:electron:watch\" \"pnpm dev:electron\"",
    "dev:nuxt": "nuxt dev --port 3000",
    "dev:electron": "wait-on http://localhost:3000 && electron --no-sandbox .",
    "dev:electron:watch": "node scripts/copy-assets.js && tsc -p electron/tsconfig.json --watch",
    "build": "pnpm build:shared && nuxt build && pnpm build:electron",
    "build:shared": "pnpm -C ../../packages/shared build",
    "build:dist": "pnpm build:shared && cross-env NUXT_TYPECHECK=false nuxt build && pnpm build:electron",
    "build:electron": "tsc -p electron/tsconfig.json && node scripts/copy-assets.js && node scripts/copy-frontend.js && node scripts/copy-package-json.js && node scripts/prepare-node-modules.mjs",
    "dist": "pnpm prebuild:nodejs && pnpm build:dist && electron-builder --publish=never",
    "clean": "node scripts/cleanup.js",
    "test": "vitest run",
    "test:e2e": "playwright test --config playwright.config.ts",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:deps": "node scripts/test-deps.js",
    "test:deps:build": "node scripts/test-deps.js --dist-electron",
    "test:deps:release": "node scripts/test-deps.js --packaged ./release/linux-unpacked/resources",
    "typecheck": "nuxt typecheck && tsc -p electron/tsconfig.json --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "nuxt preview",
    "prepare": "nuxt prepare",
    "prebuild:nodejs": "node scripts/download-nodejs.js --current-only",
    "prebuild:nodejs:all": "node scripts/download-nodejs.js"
  },
  "dependencies": {
    "@primevue/themes": "^4.5.4",
    "@suisui/shared": "workspace:*",
    "isomorphic-git": "^1.37.1",
    "keytar": "^7.9.0"
  },
  "devDependencies": {
    "@oxc-minify/binding-linux-x64-gnu": "^0.110.0",
    "@oxc-parser/binding-linux-x64-gnu": "^0.110.0",
    "@oxc-transform/binding-linux-x64-gnu": "^0.110.0",
    "@pinia/nuxt": "^0.11.3",
    "@pinia/testing": "^0.1.3",
    "@playwright/test": "^1.58.2",
    "@primevue/nuxt-module": "^4.2.0",
    "@testing-library/vue": "^8.0.0",
    "@types/node": "^20.11.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitest/coverage-v8": "^4.0.18",
    "concurrently": "^9.0.0",
    "cross-env": "^10.1.0",
    "electron": "^33.4.11",
    "electron-builder": "^25.0.0",
    "jsdom": "^24.0.0",
    "memfs": "^3.5.3",
    "nuxt": "^3.15.0",
    "primeflex": "^3.3.1",
    "primeicons": "^7.0.0",
    "primevue": "^4.2.0",
    "typescript": "^5.3.3",
    "vitest": "^2.0.0",
    "vue": "^3.5.0",
    "vue-router": "^4.5.0",
    "vue-tsc": "^3.2.4",
    "wait-on": "^8.0.0"
  },
  "build": {
    "appId": "com.suisui.desktop",
    "productName": "SuiSui",
    "artifactName": "${productName}-${version}-${arch}.${ext}",
    "npmRebuild": false,
    "directories": {
      "output": "release",
      "app": "dist-electron"
    },
    "files": [
      "**/*",
      "!__tests__/**"
    ],
    "afterPack": "scripts/after-pack.js",
    "extraResources": [
      {
        "from": "nodejs-runtime/${os}-${arch}",
        "to": "nodejs/${os}-${arch}",
        "filter": [
          "**/*"
        ]
      }
    ],
    "extraMetadata": {
      "main": "main.js"
    },
    "asar": true,
    "asarUnpack": [
      "**/*.node",
      "scripts/**"
    ],
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    },
    "linux": {
      "executableName": "suisui",
      "category": "Utility",
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "deb": {
      "packageName": "suisui-desktop"
    }
  }
}
